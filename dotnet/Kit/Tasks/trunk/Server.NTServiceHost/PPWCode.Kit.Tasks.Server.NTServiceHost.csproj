            Condition="Exists('$(OutputPath)%(distro.RecursiveDir)%(distro.Filename)%(distro.Extension)')" />
      </ItemGroup>
      <Copy 
          SourceFiles="@(forcopy)"
          DestinationFolder="$(LocalDistributionProjectRoot)$(Configuration)" />
